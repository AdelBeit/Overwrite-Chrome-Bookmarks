{
  "name": "Bookmark Overwrite",
  "version": "1.1",
  "description": "Bookmark/overwrite current tab's bookmark and removes current tab's bookmark if it's already bookmarked. \n CTRL+SHIFT+A to trigger",
  "manifest_version": 2,
  "permissions": [
    "bookmarks",
    "activeTab"
  ],
  "background":{
    "scripts": ["background.js"],
    "persistent": false
  },
  "commands":{
    "overwrite-bookmark":{
      "suggested_key": {
        "default": "Ctrl+Shift+A"
      },
      "description": "Trigger the extension"
    }
  },
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}